!!!
%html
  %head
    %meta{content: "text/html; charset=UTF-8", http: { equiv: "Content-Type"}}/
    %title Airbnb
    = favicon_link_tag('favicon.ico')
    = csrf_meta_tags
    = stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload'
    = javascript_include_tag 'application', 'data-turbolinks-track': 'reload'
  %header.fix
    .header_top
      .leftbar
        .icon
          = link_to root_path do
            %svg{ class:"top_icon", viewBox:"0 0 1000 1000", role:"presentation", aria:{hidden:"true"}, focusable:"false", style:"display: block; fill: currentcolor; height: 1em; width: 1em;"}
              %path{ d:"M499.3 736.7c-51-64-81-120.1-91-168.1-10-39-6-70 11-93 18-27 45-40 80-40s62 13 80 40c17 23 21 54 11 93-11 49-41 105-91 168.1zm362.2 43c-7 47-39 86-83 105-85 37-169.1-22-241.1-102 119.1-149.1 141.1-265.1 90-340.2-30-43-73-64-128.1-64-111 0-172.1 94-148.1 203.1 14 59 51 126.1 110 201.1-37 41-72 70-103 88-24 13-47 21-69 23-101 15-180.1-83-144.1-184.1 5-13 15-37 32-74l1-2c55-120.1 122.1-256.1 199.1-407.2l2-5 22-42c17-31 24-45 51-62 13-8 29-12 47-12 36 0 64 21 76 38 6 9 13 21 22 36l21 41 3 6c77 151.1 144.1 287.1 199.1 407.2l1 1 20 46 12 29c9.2 23.1 11.2 46.1 8.2 70.1zm46-90.1c-7-22-19-48-34-79v-1c-71-151.1-137.1-287.1-200.1-409.2l-4-6c-45-92-77-147.1-170.1-147.1-92 0-131.1 64-171.1 147.1l-3 6c-63 122.1-129.1 258.1-200.1 409.2v2l-21 46c-8 19-12 29-13 32-51 140.1 54 263.1 181.1 263.1 1 0 5 0 10-1h14c66-8 134.1-50 203.1-125.1 69 75 137.1 117.1 203.1 125.1h14c5 1 9 1 10 1 127.1.1 232.1-123 181.1-263.1z"}
        .user_page_search_box
      .rightbar
        %ul
          %li.text_holizon
            .text_vertical
              .text_wrap
                = link_to hosts_path do
                  Become a Host
          %li.text_holizon
            .text_vertical
              .text_wrap
                = link_to '#' do
                  Trips
          %li.text_holizon
            .text_vertical
              .text_wrap
                = link_to '#' do
                  Messages
          %li.text_holizon
            .text_vertical
              .text_wrap
                = link_to '#' do
                  Help
          - if user_signed_in?
            %li.user
              .user_image#user_image
                = link_to '#' do
                  = image_tag("user_pic-50x50.png")
              .menu_list
                .user_menu{style:"display: none;"}
                  %ul
                    %li
                      .menu_text
                        .menu_text_content
                          = link_to edit_user_path(id: current_user.id) do
                            Edit profile
                      .menu_text
                        .menu_text_content
                          = link_to host_path(Host.where(user_id: current_user.id).ids[0]) do
                            Host profile
                      .menu_text
                        .menu_text_content
                          = link_to destroy_user_session_path do
                            Log out
                %svg{ class:"hat", style:"display: none;"}
                  %path{ class:"white", d:"M0,10 20,10 10,0z"}
                  %path{ class:"gray_line", d:"M0,10 10,0 20,10"}
          - else
            %li.text_holizon
              .text_vertical
                .text_wrap
                  = link_to new_user_registration_path do
                    Sign up
            %li.text_holizon
              .text_vertical
                .text_wrap
                  = link_to new_user_session_path do
                    Log in
    .header_bottom
      .header_bottom_user_menu
        .header_bottom_user_menu_lists
          .header_bottom_user_menu_lists_list
            %ul
              %li
                .user_menu_list
                  .user_menu_list_text
                    = link_to '#' do
                      Dashbord
              %li
                .user_menu_list
                  .user_menu_list_text
                    = link_to '#' do
                      Inbox
              %li
                .user_menu_list
                  .user_menu_list_text
                    = link_to '#' do
                      Your List
              %li
                .user_menu_list
                  .user_menu_list_text
                    = link_to '#' do
                      Your Trips
              %li
                .user_menu_list
                  .user_menu_list_text
                    = link_to '#' do
                      Profile
              %li
                .user_menu_list
                  .user_menu_list_text
                    = link_to '#' do
                      Account
  .header_position_space
  %body
    = yield
  .footer_wrap
    %footer
      .footer_contents
        .footer_contents_main
          .footer_contents_main_top.clearfix
            .select_cur_lang
              .select_cur_lang_each
                .select_cur_lang_each_box
                  = select :name, :name, [["English", "English"], ["Deutsch", "Deutsch"], ["中文（繁體字）", "中文（繁體字）"], ["日本語", "日本語"]], {}, {class: 'select_language'}
                %span{class:"arrowtobottom"}
                  %svg{ viewBox:"0 0 18 18", role:"presentation", aria:{hidden:"true"}, focusable:"false", style:"height: 16px; width: 16px; display: block; fill: rgb(72, 72, 72);"}
                    %path{ fill:{rule:"evenodd"}, d:"M16.291 4.295a1 1 0 1 1 1.414 1.415l-8 7.995a1 1 0 0 1-1.414 0l-8-7.995a1 1 0 1 1 1.414-1.415l7.293 7.29 7.293-7.29z"}
              .select_cur_lang_each
                .select_cur_lang_each_box
                  = select :name, :name, [["USD", "USD"], ["EUR", "EUR"], ["NTD", "NTD"], ["JPY", "JPY"]], {}, {class: 'select_language'}
                %span{class:"arrowtobottom"}
                  %svg{ viewBox:"0 0 18 18", role:"presentation", aria:{hidden:"true"}, focusable:"false", style:"height: 16px; width: 16px; display: block; fill: rgb(72, 72, 72);"}
                    %path{ fill:{rule:"evenodd"}, d:"M16.291 4.295a1 1 0 1 1 1.414 1.415l-8 7.995a1 1 0 0 1-1.414 0l-8-7.995a1 1 0 1 1 1.414-1.415l7.293 7.29 7.293-7.29z"}
            .link_lists
              .link_lists_main
                %h3.font About us
                %li
                  = link_to '#' do
                    About us
            .link_lists
              .link_lists_main
                %h3.font Discover
                %li
                  = link_to '#' do
                    Trust & Safety
            .link_lists
              .link_lists_main
                %h3.font Hosting
                %li
                  = link_to '#' do
                    Why Host
          .footer_contents_main_bottom
          .footer_border
          .footer_contents_main_bottom_contents
            .footer_contents_main_bottom_contents_left
              .mini_icon
