                %li.sns
                  = link_to '#' do
                    %svg{ viewBox:"0 0 24 24", role:"img", aria:{label:"Instagram"}, focusable:"false", style:"height: 1em; width: 1em; display: block; fill: rgb(118, 118, 118);"}
