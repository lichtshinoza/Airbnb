              .company © Airbnb, Inc.
            .footer_contents_main_bottom_contents_right
              .footer_bottom_text
                %li.text
                  = link_to '#' do
                    Terms
                %li.text
                  = link_to '#' do
                    Privacy
                %li.text
                  = link_to '#' do
                    Site Map
                %li.sns
                  = link_to '#' do
                    %svg{ viewBox:"0 0 32 32", role:"img", aria:{label:"Facebook"}, focusable:"false", style:"height: 1em; width: 1em; display: block; fill: rgb(118, 118, 118);"}
                      %path{ fill:{rule:"evenodd"}, d:"M8 14.408v-4.165c0-.424.35-.812.77-.812h2.519V7.347c0-4.84 2.484-7.311 7.42-7.347 1.645 0 3.219.212 4.692.636.455.141.63.424.595.883l-.56 4.062c-.035.178-.14.354-.315.531-.21.105-.42.176-.63.14-.875-.247-1.784-.352-2.799-.352-1.399 0-1.61.283-1.61 1.73v1.8H22.6c.42 0 .805.423.805.883l-.349 4.17c0 .422-.35.705-.77.705H18.08v16c0 .424-.349.812-.769.812h-5.213c-.42 0-.804-.388-.804-.812V15.185h-2.52A.781.781 0 0 1 8 14.408"}
